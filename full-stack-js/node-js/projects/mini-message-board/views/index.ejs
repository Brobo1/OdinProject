<html>
<head>
    <title>Homepage</title>
</head>
<body>
<%- include('navbar', {links: links}) %>
<ul>
    <% messages.forEach((message, index) => { %>
        <h1><%= message.text %></h1>
        <p>Author: <%= message.user %></p>
        <p><%= message.added %></p>
        <button onclick="location.href='/message/<%=index%>';">Open</button>
    <% }) %>
</ul>
</body>
</html>